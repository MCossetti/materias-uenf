Algoritmo "1 exercício para fazer em casa"

Var
   a, b, c: inteiro
   
inicio
      escreva("Digite um valor inteiro: ")
      leia(a)
      escreva("Digite outro valor inteiro: ")
      leia (b)
      c <- a
      a <- b
      b <- c
      escreval("O novo valor de a é:", a)
      escreval("O novo valor de b é:", b)
      
fimalgoritmo
